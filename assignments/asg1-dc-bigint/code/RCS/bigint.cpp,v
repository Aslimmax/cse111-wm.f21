bigint operator+ (const bigint& lhs, const bigint& rhs) {
   bigint result = lhs;
   return result += rhs;
}

bigint operator- (const bigint& lhs, const bigint& rhs) {
   bigint result = lhs;
   return result -= rhs;
}

